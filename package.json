{
  "name": "csy-admin-ui",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm i && vite",
    "build": "npm ci && vue-tsc --build --force && vite build",
    "preview": "vite build && vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "dotenv": "16.3.1",
    "@types/lodash": "^4.14.202",
    "@types/axios": "^0.14.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.6.7",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.22.0",
    "lodash": "^4.17.21",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue-tsc": "^2.0.6"
  }
